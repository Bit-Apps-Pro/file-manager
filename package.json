{
  "name": "file-manager",
  "version": "6.4.0",
  "description": "Manage your file the way you like. You can upload, delete, copy, move, rename, compress, extract files. You don't need to worry about ftp. It is really simple and easy to use.",
  "author": "Bit Apps",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "sass:compile": "node-sass sass/style.sass -o ./assets/css --output-style expanded --source-map true",
    "sass:minify": "cleancss --format breakWith=lf --source-map --source-map-inline-sources --output ./assets/css/style.min.css ./assets/css/style.css",
    "sass:build": "npm run sass:compile && npm run sass:minify",
    "sass:watch": "npm run sass:build && onchange 'sass/**/*.sass' -- npm run sass:build",
    "build:elfinder": "./tools/build_elfinder.sh",
    "elfinder:watch": "npm run build:elfinder && onchange 'submodule/sources/elFinder/**/*' -- npm run build:elfinder",
    "build:copy": "node tools/build_copy.js",
    "build:zip": "node tools/zip_build.js",
    "build:theme": "./tools/copy_themes.js",
    "build:prod": "composer i --no-dev && pnpm i && npm run build:elfinder && npm run sass:build  && npm run build:theme && npm run build:copy && npm run build:zip",
    "i18n:php": "[ ! -d \"languages/\" ] && mkdir languages/;wp i18n make-pot ./backend languages/file-manager.pot --include=*.php --skip-js --slug=file-manager --headers='{\"Last-Translator\":\"\",\"Language-Team\":\"support@bitapps.pro\",\"PO-Revision-Date\":\"\",\"Report-Msgid-Bugs-To\":\"\"}'",
    "production": "composer i --no-dev && pnpm i && npm run i18n:php && npm run sass:build",
    "dev": "vite",
    "build": "tsc && vite build",
    "pretty": "npx prettier ./frontend/src --check",
    "pretty:fix": "npx prettier ./frontend/src --check --write",
    "ts-check": "tsc",
    "pre-commit": "lefthook run pre-commit"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.17.2",
    "@ant-design/icons": "^5.2.6",
    "@emotion/react": "^11.11.1",
    "@loadable/component": "^5.15.3",
    "@tanstack/react-query": "^4.35.3",
    "@tanstack/react-query-devtools": "^4.35.3",
    "antd": "^5.11.1",
    "framer-motion": "^10.16.4",
    "jotai": "^2.5.1",
    "lucide-react": "^0.292.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/css-modules": "^1.0.5",
    "@types/loadable__component": "^5.13.7",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "clean-css-cli": "^5.5.2",
    "concurrently": "^7.0.0",
    "detect-port": "^1.5.1",
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-module-utils": "^2.8.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-scope": "^7.2.2",
    "eslint-utils": "^3.0.0",
    "eslint-visitor-keys": "^3.4.3",
    "node-sass": "^7.0.1",
    "onchange": "^7.1.0",
    "postcss-csso": "^6.0.1",
    "prettier": "^3.0.3",
    "react-router-dom": "^6.21.3",
    "recursive-copy": "^2.0.14",
    "stylelint": "^15.11.0",
    "stylelint-config-css-modules": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.2",
    "vite": "^4.5.0",
    "vite-plugin-babel": "^1.1.3",
    "vite-plugin-pwa": "^0.16.7",
    "vite-plugin-static-copy": "^1.0.0",
    "zip-local": "^0.3.5"
  }
}
